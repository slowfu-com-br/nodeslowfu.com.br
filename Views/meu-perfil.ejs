                        <div class="linha second"></div>
                        <form action="/post" method="post">
                            <div class="form-container-input">
                                <label for="nome">Titulo</label>
                                <input class="form-input" required placeholder="exemplo@gmail.com" type="email" id="email"
                                    name="email_usuario">
                            </div>
                            <div class="form-container-input">
                                <label for="nome">Descrição</label>
                                <input class="form-input" required placeholder="Digite a descrição da sua produção" type="text" id="text"
                                    name="text_usuario">
                            </div>
                            <div class="form-container-input">
                                <label for="nome">Selecione uma Imagem</label>
                                <input class="" placeholder="Digite a descrição da sua produção" required type="file" id="img_postagem"
                                    name="imagem-postagem">
                            </div>
                            <div class="form-container-input">
                              <label for="email">Telefone</label>
                              <input class="form-input" required placeholder="(99) 99999-9999" type="tel" id="tel_tel"
                                  name="telefone_usuario">
                            </div>
                            <div class="form-container-input">
                                <label for="email">Whatsapp</label>
                                <input class="form-input" required placeholder="(99) 99999-9999" type="email" id="tel-whats"
                                    name="telefone_whatsapp">
                            </div>
                            <div class="button_contato">
                                <button class="form-input" id="yellow" type="submit">C A D A S T R A R</button>
                            </div>
                        </form>
